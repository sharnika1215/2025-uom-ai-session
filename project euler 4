max_palin = 0
factors = (0, 0)

for i in range(100, 1000):          # 3-digit numbers
    for j in range(i, 1000):        # start j from i to avoid duplicate pairs
        p = i * j
        if str(p) == str(p)[::-1]:  # check palindrome
            if p > max_palin:
                max_palin = p
                factors = (i, j)

print(max_palin)    # 906609
print(factors)      # (913, 993)
